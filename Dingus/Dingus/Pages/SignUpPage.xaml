<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Dingus.Pages.SignUpPage" Title="Sign Up">
    <Frame>
        <StackLayout>
            <Entry Placeholder="Login" IsSpellCheckEnabled="False" />
            <Entry Placeholder="Password" IsPassword="True" />
            <Entry Placeholder="Re-Password" IsPassword="True" />
            <Entry Placeholder="First Name" IsSpellCheckEnabled="False" />
            <Entry Placeholder="Last Name" IsSpellCheckEnabled="False" />
            <DatePicker />
            <Button Text="Sign Up" />
        </StackLayout>
    </Frame>
</ContentPage>